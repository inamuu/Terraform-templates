{
    "StartAt": "DynamoDB PutItem",
    "States": {
        "DynamoDB PutItem": {
            "Type": "Task",
            "Resource": "arn:aws:states:::dynamodb:putItem",
            "Parameters": {
                "TableName": "${dynamodb_table_name}",
                "Item": {
                    "eventId": {
                        "S.$": "$.id"
                    },
                    "ConditionExpression": "attribute_not_exists(eventId)"
                }
            },
            "Next": "NotifyStart",
            "Catch": [
                {
                    "ErrorEquals": [
                        "DynamoDB.ConditionalCheckFailedException"
                    ],
                    "Next": "NotifyEnd",
                    "ResultPath": "$.result"
                },
                {
                    "ErrorEquals": [
                        "States.TaskFailed"
                    ],
                    "Next": "NotifyEnd",
                    "ResultPath": "$.result"
                }
            ],
            "ResultPath": "$.result"
        },
        "NotifyStart": {
            "Type": "Task",
            "Resource": "${aws_lambda_function_slack_notify}",
            "Next": "PreProcess",
            "InputPath": "$"
        },
        "NotifyEnd": {
            "Type": "Task",
            "Resource": "${aws_lambda_function_slack_notify}",
            "InputPath": "$",
            "End": true
        },
        "PreProcess": {
            "Type": "Task",
            "Resource": "${aws_lambda_function_preprocess}",
            "Next": "DoSomething",
        },
        "DoSomething": {
            "Type": "Task",
            "Resource": "${aws_lambda_function_dosomething}",
            "Next": "Wait",
        },
        "Wait": {
            "Type": "Wait",
            "Seconds": 10,
            "Next": "NotifyEnd"
        }
    }
}